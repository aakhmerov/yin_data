    <xs:complexType name="NumberRange">
        <xs:annotation>
            <xs:documentation xml:lang="en">range for several number ranges</xs:documentation>
            <xs:documentation xml:lang="de-DE">Bereich verschiedene Nummernbereiche</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="minimal" type="xs:nonNegativeInteger" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">minimal number</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Minimale Nummer</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element
                    name="maximal" type="xs:nonNegativeInteger" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">maximal number</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Maximale Nummer</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:all>
    </xs:complexType>

    <xs:simpleType name="DMYDate">
        <xs:annotation>
            <xs:documentation xml:lang="en">date formatted "dd.mm.yyyy"</xs:documentation>
            <xs:documentation xml:lang="de-DE">Datum im Format "dd.mm.yyyy"</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-3]{1}[0-9]{1}.[0,1]{1}[0-9]{1}.[0-9]{4}"/>
        </xs:restriction>
    </xs:simpleType>

 

 


